<h1>Word Central</h1>

<h2>Submit text for Ascii</h2>

<!-- ***** Updated ***** -->
<form action="/asciis" method="post">
  <div>
    <label for="ascii_user_text">Text</label>
    <textarea name="ascii[user_text]" id="ascii_user_text"></textarea>
